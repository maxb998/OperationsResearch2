    "version": "0.2.0",
    "configurations": [
        {
            "type":"cppdbg",
            "name": "Debug",
            "preLaunchTask": "Build Debug",
            "request": "launch",
            "program": "${workspaceFolder}/bin/debug/main",
            "args": [
                "--file", "data/a280.tsp",
                "--mode", "tabu",

                "--graspType", "almostbest",
                "--graspChance", "0.01",
                //"--2opt",
                "--tlim", "10",

                //"--metaInit", "em",
                //"--cplexInit", "em",

                //"--nnTryall",
                //"--emFarthest",
                //"--tabuTenureSize", "5",
                //"--metaRestartThreshold", "1000",
                //"--hardfixSmallest",

                //"--seed", "2023",
                "--threads", "8",
                "--roundcosts",
                //"--plot", 
                //"--save",
                "--loglvl", "log"
            ],
            "cwd": "${workspaceFolder}"
       },
       {
            "type":"cppdbg",
            "name": "Exec",
            "preLaunchTask": "Build Exec",
            "request": "launch",
            "program": "${workspaceFolder}/bin/x64/main",
            "args": [
                "--file", "data/a280.tsp",
                "--mode", "tabu",

                "--graspType", "almostbest",
                "--graspChance", "0.01",
                //"--2opt",
                "--tlim", "10",

                //"--metaInit", "em",
                //"--cplexInit", "em",

                //"--nnTryall",
                //"--emFarthest",
                //"--tabuTenureSize", "5",
                //"--metaRestartThreshold", "1000",
                //"--hardfixSmallest",

                //"--seed", "2023",
                "--threads", "8",
                "--roundcosts",
                //"--plot", 
                //"--save",
                "--loglvl", "log"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}
